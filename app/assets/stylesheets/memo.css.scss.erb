div#login{
  height: 250px;
  background: url(<%= 'inicio/fondo.png' %>)
}

div#cabecera{
  height: 50px;
  background: url(<%= asset_path 'chat/cabecera.png' %>) no-repeat;
  color: #ffffff;
}

div#usuario{
  margin-top: 10px;
}

div#barra{
  height: 6px;
  background: url(<%= asset_path 'chat/barra.png' %>) no-repeat;
  margin: 1px 0px;
}

div#principal{
  height: 415px;
  background: url(<%= asset_path 'chat/fondo.png' %>) no-repeat;
}

div#player{
  height: 163px;
  background: #c3d2de;
}

div#emoticon{
  margin: 10px;
}

div#chat{
  height: 250px;;
  background-color:#ffffff;
  overflow:auto;
  border: 1px solid #808080;
  padding: 5px 10px;
}

div#mensaje input.texto{
  width: 270px; 
  border: 1px solid #808080;
}

div#contactos{
  background: white;
  opacity: .65;
  filter: alpha(opacity=65);
}


div#lista-contactos{
  background: #eeeeee;
}

div#lista-contactos ul{
  list-style-image: url(<%= asset_path 'chat/vinieta.png' %>)
}
